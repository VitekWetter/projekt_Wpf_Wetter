<Window x:Class="pujcovnaHudebnichNastroju.login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pujcovnaHudebnichNastroju"
        mc:Ignorable="d"
        Title="login" Height="450" Width="800">
    <Grid>
        <Button x:Name="loginBtn" Content="Přihlásit se" FontSize="30" HorizontalAlignment="Left" Margin="494,200,0,0" VerticalAlignment="Top" Click="loginBtn_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="84,120,0,0" TextWrapping="Wrap" Text="Jméno" FontSize="20" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="84,220,0,0" TextWrapping="Wrap" Text="Heslo" FontSize="20" VerticalAlignment="Top"/>
        <TextBox x:Name="logJmeno" HorizontalAlignment="Left" FontSize="15" Margin="81,160,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="202" Height="29"/>
        <PasswordBox x:Name="logHeslo" HorizontalAlignment="Left" FontSize="15" Margin="81,260,0,0" VerticalAlignment="Top" Width="202" Height="29"/>
    </Grid>
</Window>
